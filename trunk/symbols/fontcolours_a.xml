<?xml version='1.0' encoding='us-ascii'?>

<tunnelxml>

<fontcolours fontstyle="PLAIN" fontname="Serif">


<groupsubsetattr groupsubsetname="baseSymbols250" groupsubsetselectable="no">
	<subsetattr name="default"/>
	<subsetattr name="visiblesets">
		<set multiplicity="1" buttonaction="overwrite" area-interaction="no-overlap">
			<symbolaut dname="slope" description="slope symbol">
				<asymbol name="slope" position="endpath" scale="fixed" picscale="0.5" orientation="alongaxis" area-interaction="allowed-outside" drawstyle="filled"/>
			</symbolaut>
			<symbolaut dname="stream" description="stream symbol">
				<asymbol name="stream" position="endpath" scale="fixed" picscale="0.5" orientation="nearaxis" area-interaction="allowed-outside"/>
			</symbolaut>

			<symbolaut dname="column" description="Stal column">
				<asymbol name="column" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="curtain" description="Stal curtain">
				<asymbol name="curtain" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="stalactite" description="Stalactite">
				<asymbol name="stalactite" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="stalagmite" description="Stalagmite">
				<asymbol name="stalagmite" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="straws" description="Straws">
				<asymbol name="straws" position="endpath" scale="fixed" orientation="fixed"/>
			</symbolaut>

			<symbolaut dname="mud" buttonaction="append" description="muddy area">
				<asymbol name="mud" position="random" scale="fixed" picscale="0.5" orientation="fixed" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="pebbles" buttonaction="append" description="some pebbles">
				<asymbol name="pebble" position="random" scale="fixed" orientation="fixed" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="puddle" description="9x9puddle">
				<asymbol name="puddle" position="lattice" scale="fixed" orientation="fixed" multiplicity="fill" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="sand" buttonaction="append" description="lots of sand">
				<asymbol name="sand" position="random" scale="fixed" picscale="0.5" orientation="fixed" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="sump" description="9x9sump">
				<asymbol name="sump" position="lattice" scale="fixed" orientation="fixed" multiplicity="fill" area-interaction="trim"/>
			</symbolaut>

			<symbolaut dname="breeze" description="breeze symbol">
				<asymbol name="breeze" position="endpath" scale="fixed" picscale="0.5" orientation="alongaxis" area-interaction="allowed-outside"/>
			</symbolaut>

			<symbolaut dname="boulders" buttonaction="append" picscale="0.5" description="lots of boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="fill"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="fill"/>
			</symbolaut>
		</set>

		<set multiplicity="fill" buttonaction="overwrite" scale="fixed">
			<symbolaut dname="flowstone" description="calcity area">
				<asymbol name="flowstone" position="random" picscale="0.5" area-interaction="no-overlap" orientation="alongaxis"/>
			</symbolaut>
			<symbolaut dname="flowperp" position="random" picscale="0.5" orientation="closestfromaxis" area-interaction="no-overlap" description="flowstone perpendicular">
				<asymbol name="flowstone"/>
			</symbolaut>
			<symbolaut dname="flowpara" position="random" picscale="0.5" orientation="closestalongaxis" area-interaction="no-overlap" description="flowstone parallel">
				<asymbol name="flowstone"/>
			</symbolaut>
		</set>
	</subsetattr>
</groupsubsetattr>


