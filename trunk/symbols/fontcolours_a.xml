			<symbolaut dname="neckbould" axisscale="0.3" axisscaleperp="30.0" position="alongpath_random_pullback" scale="andhalf" orientation="random" area-interaction="no-overlap" description="boulder necklace">
				<asymbol name="boulder"/>
