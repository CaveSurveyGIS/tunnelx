				<asymbol name="sump"/>
			</symbolaut>
		</set>

		<set multiplicity="fill" buttonaction="overwrite">
			<symbolaut dname="flowstone" description="calcity area">
				<asymbol name="flowstone" scale="fixed" picscale="1.5" area-interaction="no-overlap" orientation="alongaxis"/>
			</symbolaut>
			<symbolaut dname="neckstream" axisscale="2.0" position="alongpath_even" scale="fixed" orientation="alongaxis" area-interaction="no-overlap" description="stream necklace">
