<?xml version='1.0' encoding='us-ascii'?>

<tunnelxml>

<fontcolours fontstyle="PLAIN" fontname="Serif">

<area_signal_def asigname="plan" asigeffect="keeparea"/>
<area_signal_def asigname="rock" asigeffect="killarea"/>
<area_signal_def asigname="hole" asigeffect="outlinearea"/>
<area_signal_def asigname="verysteep" asigeffect="keeparea"/>
<area_signal_def asigname="dropdown" asigeffect="hcoincide"/>

<groupsubsetattr groupsubsetname="base" groupsubsetselectable="no">
	<grid_def xorig="0.0" yorig="0.0" maxgridlines="8">
		<gridspacing gswidth="1.0" maxgridlines="13"/>
		<gridspacing gswidth="5.0"/>
		<gridspacing gswidth="10.0"/>
		<gridspacing gswidth="50.0"/>
		<gridspacing gswidth="100.0"/>
		<gridspacing gswidth="500.0"/>
	</grid_def>
	<subsetattr name="basevisiblecave" areamaskcolour="#7bffffff" areacolour="#66cce5e5" strokecolour="#ff000000" fontstyle="PLAIN" labelcolour="$labcolour">
		<setvariable name="$labcolour" value="#ff000000"/>
		<labelfcol labelstylename="default"/>
		<labelfcol labelstylename="qm" size="9.375"/>
		<labelfcol labelstylename="step" fontstyle="ITALIC" size="12.5"/>
		<labelfcol labelstylename="STEP" size="12.5"/>
		<labelfcol labelstylename="passage" fontstyle="BOLD" size="23"/>
		<labelfcol labelstylename="entrance" fontname="Franklin Gothic Medium" size="30.0"/>
		<labelfcol labelstylename="area" fontname="Franklin Gothic Medium" fontstyle="ITALIC" size="34.375"/>
		<labelfcol labelstylename="cave" fontname="Franklin Gothic Medium" fontstyle="BOLD" size="96.375"/>

		<linestylecol linestyle="centreline" strokewidth="0.312" strokecolour="#ff800000"/>
		<linestylecol linestyle="wall" strokewidth="1.25"/>
		<linestylecol linestyle="estwall" strokewidth="1.25" spikegap="7.5" gapleng="3.75" spikeheight="0.0" strokecolour="#ff000000"/>
		<linestylecol linestyle="pitchbound" strokewidth="0.625" spikegap="7.5" gapleng="0.0" spikeheight="2.5"/>
		<linestylecol linestyle="ceilingbound" strokewidth="0.625" spikegap="7.5" gapleng="2.5" spikeheight="2.5"/>
		<linestylecol linestyle="detail" strokewidth="0.625"/>
		<linestylecol linestyle="filled" strokewidth="0.0" strokecolour="#ff000080"/>

		<set multiplicity="1" buttonaction="overwrite" area-interaction="no-overlap">

			<symbolaut dname="slope" description="slope symbol">
				<asymbol name="slope" position="endpath" scale="fixed" orientation="alongaxis" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="breeze" description="breeze symbol">
				<asymbol name="breeze" position="endpath" scale="fixed" orientation="alongaxis" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="stream" description="stream symbol">
				<asymbol name="stream" position="endpath" scale="fixed" orientation="nearaxis" area-interaction="allowed-outside"/>
			</symbolaut>

			<symbolaut dname="stalagmite" description="Stalagmite">
				<asymbol name="stalagmite" position="endpath" scale="fixed" orientation="fixed" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="stalagtite" description="Misspelled stalactite">
				<asymbol name="stalagtite" position="endpath" scale="fixed" orientation="fixed" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="column" description="Stal column">
				<asymbol name="column" position="endpath" scale="fixed" orientation="fixed" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="curtain" description="Stal curtain">
				<asymbol name="curtain" position="endpath" scale="fixed" orientation="fixed" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="straws" description="Straws">
				<asymbol name="straws" position="endpath" scale="fixed" orientation="fixed" area-interaction="trim"/>
			</symbolaut>

			<symbolaut dname="bignorth" description="North Arrow">
				<asymbol name="northarrowbig" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="smallnorth" description="Small North Arrow">
				<asymbol name="northarrowsmall" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="varnorth" description="Resizeable North Arrow">
				<asymbol name="northarrowsmall" position="endpath" scale="scaleable" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>

			<symbolaut dname="10mscale" description="10m scale bar">
				<asymbol name="scalebar10m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="30mscale" description="30m scale bar">
				<asymbol name="scalebar30m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="50mscale" description="50m scale bar">
				<asymbol name="scalebar50m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="100mscale" description="100m scale bar">
				<asymbol name="scalebar100m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>

			<symbolaut dname="snow" description="9x9snow">
				<asymbol name="snow" position="lattice" scale="fixed" orientation="fixed" multiplicity="441" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="puddle" description="9x9puddle">
				<asymbol name="puddle" position="lattice" scale="fixed" orientation="fixed" multiplicity="441" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="sump" description="9x9sump">
				<asymbol name="sump" position="lattice" scale="fixed" orientation="fixed" multiplicity="441" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="hexmud" description="cracked mud">
				<asymbol name="hexagons" position="lattice" scale="fixed" orientation="fixed" multiplicity="441" area-interaction="trim"/>
			</symbolaut>

			<symbolaut dname="sand" buttonaction="append" description="lots of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="sand30" buttonaction="append" description="30 grains of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="30"/>
			</symbolaut>
			<symbolaut dname="sand100" buttonaction="append" description="100 grains of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="100"/>
			</symbolaut>
			<symbolaut dname="sand500" buttonaction="append" description="500 grains of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="500"/>
			</symbolaut>
			<symbolaut dname="boulder" buttonaction="append" description="boulder">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="1"/>
			</symbolaut>
			<symbolaut dname="boulders" buttonaction="append" description="lots of boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="fill"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="5boulder" buttonaction="append" description="5 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="2"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="3"/>
			</symbolaut>
			<symbolaut dname="10boulder" buttonaction="append" description="10 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="4"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="6"/>
			</symbolaut>
			<symbolaut dname="30boulder" buttonaction="append" description="30 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="12"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="18"/>
			</symbolaut>
			<symbolaut dname="100boulder" buttonaction="append" description="100 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="40"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="60"/>
			</symbolaut>

			<symbolaut dname="bigboulder" buttonaction="append" description="boulder">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="1"/>
			</symbolaut>
			<symbolaut dname="bigboulders" buttonaction="append" description="lots of big boulders">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="fill"/>
				<asymbol name="bigboulder2" position="random" scale="andhalf" orientation="random" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="5bigboulder" buttonaction="append" description="5 boulders">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="2"/>
				<asymbol name="bigboulder2" position="random" scale="andhalf" orientation="random" multiplicity="3"/>
			</symbolaut>
			<symbolaut dname="10bigboulder" buttonaction="append" description="10 boulders">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="4"/>
				<asymbol name="bigboulder2" position="random" scale="andhalf" orientation="random" multiplicity="6"/>
			</symbolaut>

			<symbolaut dname="pebbles" buttonaction="append" description="some pebbles">
				<asymbol name="pebble" position="random" scale="fixed" orientation="fixed" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="50pebbles" buttonaction="append" description="lots of pebbles">
				<asymbol name="pebble" position="random" scale="fixed" orientation="fixed" multiplicity="fill"/>
			</symbolaut>
			<symbolaut dname="mud" buttonaction="append" description="muddy area">
				<asymbol name="mud" position="random" scale="fixed" orientation="fixed" multiplicity="fill"/>
			</symbolaut>
		</set>
	</subsetattr>

	<subsetattr name="baseinvisiblecave" areamaskcolour="none" areacolour="none" labelcolour="none" strokecolour="none" fontstyle="PLAIN" size="1" strokewidth="0.1">
		<labelfcol labelstylename="default"/>
		<labelfcol labelstylename="qm"/>
		<labelfcol labelstylename="step"/>
		<labelfcol labelstylename="STEP"/>
		<labelfcol labelstylename="passage"/>
		<labelfcol labelstylename="area"/>
		<labelfcol labelstylename="entrance"/>
		<labelfcol labelstylename="cave"/>

		<linestylecol linestyle="centreline"/>
		<linestylecol linestyle="wall"/>
		<linestylecol linestyle="estwall"/>
		<linestylecol linestyle="pitchbound"/>
		<linestylecol linestyle="ceilingbound"/>
		<linestylecol linestyle="detail"/>
		<linestylecol linestyle="filled" strokewidth="0.0"/>

		<set multiplicity="1" buttonaction="overwrite" area-interaction="no-overlap">
			<symbolaut dname="slope" description="slope symbol"></symbolaut>
			<symbolaut dname="breeze" description="breeze symbol"></symbolaut>
			<symbolaut dname="stream" description="stream symbol"></symbolaut>
			<symbolaut dname="sand" buttonaction="append" description="lots of sand"></symbolaut>
			<symbolaut dname="sand30" buttonaction="append" description="30 grains of sand"></symbolaut>
			<symbolaut dname="sand100" buttonaction="append" description="100 grains of sand"></symbolaut>
			<symbolaut dname="sand500" buttonaction="append" description="500 grains of sand"></symbolaut>
			<symbolaut dname="boulder" buttonaction="append" description="boulder"></symbolaut>
			<symbolaut dname="boulders" buttonaction="append" description="lots of boulders"></symbolaut>
			<symbolaut dname="5boulder" buttonaction="append" description="5 boulders"></symbolaut>
			<symbolaut dname="10boulder" buttonaction="append" description="10 boulders"></symbolaut>
			<symbolaut dname="30boulder" buttonaction="append" description="30 boulders"></symbolaut>
			<symbolaut dname="100boulder" buttonaction="append" description="100 boulders"></symbolaut>
			<symbolaut dname="snow" description="9x9snow"></symbolaut>
			<symbolaut dname="puddle" description="9x9puddle"></symbolaut>
			<symbolaut dname="sump" description="9x9sump"></symbolaut>
			<symbolaut dname="bigboulder" buttonaction="append" description="boulder"></symbolaut>
			<symbolaut dname="bigboulders" buttonaction="append" description="lots of big boulders"></symbolaut>
			<symbolaut dname="5bigboulder" buttonaction="append" description="5 boulders"></symbolaut>
			<symbolaut dname="10bigboulder" buttonaction="append" description="10 boulders"></symbolaut>
			<symbolaut dname="stalagmite" description="Stalagmite"></symbolaut>
			<symbolaut dname="choke" description="Choke"></symbolaut>
		</set>
	</subsetattr>
	
	
	<subsetattr name="basevisiblegrid" areamaskcolour="#f0ffffff" areacolour="#30ff0000" strokecolour="#ff000000"
				fontstyle="PLAIN" labelcolour="#ff0030e0">
		<labelfcol labelstylename="default"/>
        	<labelfcol labelstylename="titlenum" fontname="Franklin Gothic Medium" size="60"/>
        	<labelfcol labelstylename="titlename" fontname="Franklin Gothic Medium" fontstyle="BOLD" size="120"/>
        	<labelfcol labelstylename="titlearea" fontname="Franklin Gothic Medium" fontstyle="BOLD" size="40"/>
        	<labelfcol labelstylename="titleentname" fontname="Times New Roman" fontstyle="BOLD" size="60"/>
        	<labelfcol labelstylename="titleentpos" fontname="Times New Roman" size="60"/>
        	<labelfcol labelstylename="titlestats" fontname="Times New Roman" fontstyle="BOLD" size="50"/>
        	<labelfcol labelstylename="titledetails" fontname="Times New Roman" size="50"/>
        	<labelfcol labelstylename="gridnum" fontname="Franklin Gothic Medium" size="40"/>

		<labelfcol labelstylename="titlecreditssmall" fontname="Serif" size="10"/>
		<labelfcol labelstylename="titlecreditslarge" fontname="Serif" size="35"/>

		<linestylecol linestyle="detail" strokewidth="0.8" strokecolour="#ff800080"/>
		<linestylecol linestyle="wall" strokewidth="3.5" strokecolour="#ff800080"/>
		<set multiplicity="1" buttonaction="overwrite" area-interaction="no-overlap">
			<symbolaut dname="bignorth" description="North Arrow">
				<asymbol name="northarrowbig" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="smallnorth" description="Small North Arrow">
				<asymbol name="northarrowsmall" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="varnorth" description="Resizeable North Arrow">
				<asymbol name="northarrowsmall" position="endpath" scale="scaleable" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="100mscale" description="100m scale bar">
				<asymbol name="scalebar100m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="10mscale" description="10m scale bar">
				<asymbol name="scalebar10m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="300mscale" description="300m scale bar">
				<asymbol name="scalebar30m" picscale="10.0" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="50mscale" description="50m scale bar">
				<asymbol name="scalebar50m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
		</set>
	</subsetattr>

	<subsetattr name="baseinvisiblegrid" areamaskcolour="none" areacolour="none" labelcolour="none" 
			strokecolour="none" fontstyle="PLAIN" size="1" strokewidth="0.1">
				<labelfcol labelstylename="default"/>
        	<labelfcol labelstylename="titlenum"/>
        	<labelfcol labelstylename="titlename"/>
        	<labelfcol labelstylename="titlearea"/>
        	<labelfcol labelstylename="titleentname"/>
        	<labelfcol labelstylename="titleentpos"/>
        	<labelfcol labelstylename="titlestats"/>
        	<labelfcol labelstylename="titledetails"/>
        	<labelfcol labelstylename="gridnum"/>

		<labelfcol labelstylename="titlecreditssmall"/>
		<labelfcol labelstylename="titlecreditslarge"/>

		<linestylecol linestyle="detail"/>
		<linestylecol linestyle="wall"/>
		<set multiplicity="1" buttonaction="overwrite" area-interaction="no-overlap">
			<symbolaut dname="bignorth" description="North Arrow"></symbolaut>
			<symbolaut dname="smallnorth" description="Small North Arrow"></symbolaut>
			<symbolaut dname="varnorth" description="Resizeable North Arrow"></symbolaut>
			<symbolaut dname="100mscale" description="100m scale bar"></symbolaut>
			<symbolaut dname="10mscale" description="10m scale bar"></symbolaut>
			<symbolaut dname="300mscale" description="300m scale bar"></symbolaut>
			<symbolaut dname="50mscale" description="50m scale bar"></symbolaut>
		</set>
	</subsetattr>	

	<subsetattr name="default" uppersubset="basevisiblecave"/>
</groupsubsetattr>


<groupsubsetattr groupsubsetname="base500" groupsubsetselectable="no">
	<importgroupsubsetattr groupsubsetname="base"/>
</groupsubsetattr>

<groupsubsetattr groupsubsetname="base1000" groupsubsetselectable="no">
	<importgroupsubsetattr groupsubsetname="base"/>
	<subsetattr name="basevisiblecave">
		<labelfcol labelstylename="qm" size="40.0" labelcolour="#ff00bf00"/>
		<labelfcol labelstylename="step" fontstyle="ITALIC" size="40.0"/>
		<labelfcol labelstylename="passage" fontstyle="BOLD" size="90.0"/>
		<labelfcol labelstylename="feature" size="60.0"/>
		<labelfcol labelstylename="entrance" fontname="Franklin Gothic Medium" size="60.0"/>
		<labelfcol labelstylename="area" fontname="Franklin Gothic Medium" fontstyle="ITALIC" size="95.0"/>
		<labelfcol labelstylename="cave" fontname="Franklin Gothic Medium" fontstyle="BOLD" size="96.375"/>
		<labelfcol labelstylename="description" size="5.0"/>

		<linestylecol linestyle="centreline" strokewidth="0.312" strokecolour="#ff800000"/>
		<linestylecol linestyle="wall" strokewidth="2.25"/>
		<linestylecol linestyle="estwall" strokewidth="2.25" spikegap="7.5" gapleng="3.75" spikeheight="0.0"/>
		<linestylecol linestyle="pitchbound" strokewidth="1.225" spikegap="9.5" gapleng="0.0" spikeheight="4.5"/>
		<linestylecol linestyle="ceilingbound" strokewidth="1.225" spikegap="9.5" gapleng="2.5" spikeheight="4.0"/>
		<linestylecol linestyle="detail" strokewidth="1.225"/>
		<linestylecol linestyle="filled" strokewidth="0.0" strokecolour="#ff000080"/>


		<set multiplicity="1" buttonaction="overwrite" position="endpath" scale="fixed" orientation="alongaxis" area-interaction="allowed-outside">
			<symbolaut dname="slope" description="slope symbol">
				<asymbol name="slope" picscale="1.5"/>
			</symbolaut>
			<symbolaut dname="slope2" description="big slope symbol">
				<asymbol name="slope" picscale="3.0"/>
			</symbolaut>
			<symbolaut dname="breeze" description="breeze symbol">
				<asymbol name="breeze" picscale="2.8"/>
			</symbolaut>
			<symbolaut dname="stream" description="stream symbol">
				<asymbol name="stream" picscale="2.5"/>
			</symbolaut>
			<symbolaut dname="stream2" description="big stream symbol">
				<asymbol name="stream" picscale="5.0"/>
			</symbolaut>
			<symbolaut dname="npit" description="nitrate pit">
				<asymbol name="npit" scale="alongaxis" />
			</symbolaut>

			<symbolaut dname="popcorn" description="popcorn formations">
				<asymbol name="popcorn" picscale="4.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="stalagmite" description="stalagmite formation">
				<asymbol name="stalagmite" picscale="4.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="stalagtite" description="stalagmite formation">
				<asymbol name="stalagtite" picscale="4.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="column" description="stalagmite formation">
				<asymbol name="column" picscale="4.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="curtain" description="curtain formation">
				<asymbol name="curtain" picscale="4.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="straws" description="straw formations">
				<asymbol name="straws" picscale="4.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="bedrock" description="bare solid bedrock">
				<asymbol name="bedrock" picscale="3.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="bedrock2" description="big bare solid bedrock">
				<asymbol name="bedrock" picscale="6.0" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="rats" description="rat activity">
				<asymbol name="rats" picscale="2.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="guano" description="bat shit">
				<asymbol name="bats" picscale="2.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="humact" description="human activity">
				<asymbol name="pants" picscale="2.5" orientation="fixed"/>
			</symbolaut>
		</set>

		<set multiplicity="fill" buttonaction="append" position="random" area-interaction="no-overlap" picscale="2.5" scale="fixed" orientation="fixed">
			<symbolaut dname="sand" description="lots of sand">
				<asymbol name="sand" scale="fixed" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="pebbles" description="pebbly area">
				<asymbol name="pebble" scale="fixed" picscale="1.0" orientation="random"/>
			</symbolaut>
			<symbolaut dname="mud" description="muddy area">
				<asymbol name="mud" scale="andhalf" picscale="1.5" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="boulders" buttonaction="append" picscale="1.5" description="lots of boulders">
				<asymbol name="boulder" scale="andhalf" orientation="random"/>
			</symbolaut>
			<symbolaut dname="bigboulders" buttonaction="append" picscale="3.5" description="lots of big boulders">
				<asymbol name="boulder" scale="andhalf" orientation="random"/>
			</symbolaut>
			<symbolaut dname="flowstone" description="calcity area">
				<asymbol name="flowstone" scale="fixed" picscale="1.5" orientation="alongaxis"/>
			</symbolaut>


			<symbolaut dname="sand30" description="30 grains of sand">
				<asymbol name="sand" multiplicity="30" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="10boulder" description="10 boulders" scale="andhalf" orientation="random">
				<asymbol name="boulder" multiplicity="4"/>
				<asymbol name="triangleboulder" multiplicity="6"/>
			</symbolaut>
		</set>

		<set multiplicity="fill" position="latticef" area-interaction="trim" buttonaction="overwrite" picscale="2.5" scale="fixed" orientation="fixed">
			<symbolaut dname="puddle" description="water area">
				<asymbol name="puddle"/>
			</symbolaut>
			<symbolaut dname="hexmud" picscale="3.5" description="fractured mud area">
				<asymbol name="hexagons"/>
			</symbolaut>
			<symbolaut dname="sump" description="9x9sump">
				<asymbol name="sump"/>
			</symbolaut>
		</set>
	</subsetattr>

	<subsetattr name="basevisiblegrid" areacolour="#30eeddee" labelcolour="#ff1100e0">
	<labelfcol labelstylename="default"/>
        <labelfcol labelstylename="keytext" fontname="Franklin Gothic Medium" size="80"/>

        <labelfcol labelstylename="titlenamechinese" fontstyle="BOLD" size="1100"/>
        <labelfcol labelstylename="titlename" fontname="Franklin Gothic Medium" fontstyle="BOLD" size="500"/>
        <labelfcol labelstylename="titlestats" fontname="Times New Roman" size="220"/>
	</subsetattr>

</groupsubsetattr>

<groupsubsetattr groupsubsetname="default">
	<importgroupsubsetattr groupsubsetname="base500"/>
	<subsetattr name="visible" uppersubset="basevisiblecave">
		<setvariable name="$labcolour" value="#ff011110"/>
	</subsetattr>
	<subsetattr name="invisible" uppersubset="baseinvisiblecave"/>
	
	<subsetattr name="visiblegrid" uppersubset="basevisiblegrid"/>
