<?xml version='1.0' encoding='us-ascii'?>

<tunnelxml>

<fontcolours fontstyle="PLAIN" fontname="Serif">

<groupsubsetattr groupsubsetname="default">

	<subsetattr name="default" areamaskcolour="#8cffffff" areacolour="#66cce5e5" strokecolour="#ff000000"
				fontstyle="PLAIN" labelcolour="#ff000000">
		<labelfcol labelstylename="default"/>
		<labelfcol labelstylename="qm" size="9.375" labelcolour="#ff00ff00"/>
		<labelfcol labelstylename="step" size="12.5"/>
		<labelfcol labelstylename="STEP" size="12.5"/>
		<labelfcol labelstylename="passage" fontstyle="BOLD" size="23"/>
		<labelfcol labelstylename="area" size="23" labelcolour="#ff00000f"/>

		<labelfcol labelstylename="entrance" fontname="Frankin Gothic Medium" size="30.0"/>
		<labelfcol labelstylename="area" fontname="Frankin Gothic Medium" fontstyle="ITALIC" size="34.375"/>
		<labelfcol labelstylename="cave" fontname="Frankin Gothic Medium" fontstyle="BOLD" size="96.375"/>

		<linestylecol linestyle="centreline" strokewidth="0.312" strokecolour="#ff800000"/>
		<linestylecol linestyle="wall" strokewidth="1.25"/>
		<linestylecol linestyle="estwall" strokewidth="1.25" spikegap="7.5" gapleng="3.75" spikeheight="0.0" strokecolour="#ffa0a000"/>
		<linestylecol linestyle="pitchbound" strokewidth="0.625" spikegap="7.5" gapleng="0.0" spikeheight="2.5"/>
		<linestylecol linestyle="ceilingbound" strokewidth="0.625" spikegap="7.5" gapleng="2.5" spikeheight="2.5"/>
		<linestylecol linestyle="detail" strokewidth="0.625"/>
		<linestylecol linestyle="filled" strokecolour="#ff000080"/>

		<set multiplicity="1" buttonaction="overwrite" area-interaction="no-overlap">
			<symbolaut dname="slope" description="slope symbol">
				<asymbol name="slope" position="endpath" scale="fixed" orientation="alongaxis" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="breeze" description="breeze symbol">
				<asymbol name="breeze" position="endpath" scale="fixed" orientation="alongaxis" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="stream" description="stream symbol">
				<asymbol name="stream" position="endpath" scale="fixed" orientation="nearaxis" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="sand" buttonaction="append" description="lots of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="5000"/>
			</symbolaut>
			<symbolaut dname="sand30" buttonaction="append" description="30 grains of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="30"/>
			</symbolaut>
			<symbolaut dname="sand100" buttonaction="append" description="100 grains of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="100"/>
			</symbolaut>
			<symbolaut dname="sand500" buttonaction="append" description="500 grains of sand">
				<asymbol name="sand" position="random" scale="fixed" orientation="fixed" multiplicity="500"/>
			</symbolaut>
			<symbolaut dname="boulder" buttonaction="append" description="boulder">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="1"/>
			</symbolaut>
			<symbolaut dname="boulders" buttonaction="append" description="lots of boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="200"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="600"/>
			</symbolaut>
			<symbolaut dname="5boulder" buttonaction="append" description="5 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="2"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="3"/>
			</symbolaut>
			<symbolaut dname="10boulder" buttonaction="append" description="10 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="4"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="6"/>
			</symbolaut>
			<symbolaut dname="30boulder" buttonaction="append" description="30 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="12"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="18"/>
			</symbolaut>
			<symbolaut dname="100boulder" buttonaction="append" description="100 boulders">
				<asymbol name="boulder" position="random" scale="andhalf" orientation="random" multiplicity="40"/>
				<asymbol name="triangleboulder" position="random" scale="andhalf" orientation="random" multiplicity="60"/>
			</symbolaut>
			<symbolaut dname="snow" description="9x9snow">
				<asymbol name="snow" position="lattice" scale="fixed" orientation="fixed" multiplicity="441" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="puddle" description="9x9puddle">
				<asymbol name="puddle" position="lattice" scale="fixed" orientation="fixed" multiplicity="441" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="sump" description="9x9sump">
				<asymbol name="sump" position="lattice" scale="fixed" orientation="fixed" multiplicity="441" area-interaction="trim"/>
			</symbolaut>
			<symbolaut dname="bigboulder" buttonaction="append" description="boulder">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="1"/>
			</symbolaut>
			<symbolaut dname="bigboulders" buttonaction="append" description="lots of big boulders">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="20"/>
				<asymbol name="bigboulder2" position="random" scale="andhalf" orientation="random" multiplicity="30"/>
			</symbolaut>
			<symbolaut dname="5bigboulder" buttonaction="append" description="5 boulders">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="2"/>
				<asymbol name="bigboulder2" position="random" scale="andhalf" orientation="random" multiplicity="3"/>
			</symbolaut>
			<symbolaut dname="10bigboulder" buttonaction="append" description="10 boulders">
				<asymbol name="bigboulder1" position="random" scale="andhalf" orientation="random" multiplicity="4"/>
				<asymbol name="bigboulder2" position="random" scale="andhalf" orientation="random" multiplicity="6"/>
			</symbolaut>
			<symbolaut dname="stalagmite" description="Stalagmite">
				<asymbol name="stalagmite" position="endpath" scale="fixed" orientation="fixed"/>
			</symbolaut>
			<symbolaut dname="choke" description="Choke">
				<asymbol name="choke" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>

			<symbolaut dname="bignorth" description="North Arrow">
				<asymbol name="northarrowbig" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="smallnorth" description="Small North Arrow">
				<asymbol name="northarrowsmall" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="100mscale" description="100m scale bar">
				<asymbol name="scalebar100m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="10mscale" description="10m scale bar">
				<asymbol name="scalebar10m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
			<symbolaut dname="30mscale" description="30m scale bar">
				<asymbol name="scalebar30m" position="endpath" scale="fixed" orientation="fixed" area-interaction="allowed-outside"/>
			</symbolaut>
		</set>
	</subsetattr>


	<subsetattr name="grid" areamaskcolour="#f0ffffff" areacolour="#30ff0000" strokecolour="#ff0000f0"
				fontstyle="PLAIN" labelcolour="#ff0030e0">
		<labelfcol labelstylename="default"/>
        <labelfcol labelstylename="titlenum" fontname="Frankin Gothic Medium" size="60"/>
        <labelfcol labelstylename="titlename" fontname="Frankin Gothic Medium" fontstyle="BOLD" size="120"/>
        <labelfcol labelstylename="titlearea" fontname="Frankin Gothic Medium" fontstyle="BOLD" size="40"/>
        <labelfcol labelstylename="titleentname" fontname="Times New Roman" fontstyle="BOLD" size="60"/>
        <labelfcol labelstylename="titleentpos" fontname="Times New Roman" size="60"/>
        <labelfcol labelstylename="titlestats" fontname="Times New Roman" fontstyle="BOLD" size="50"/>
        <labelfcol labelstylename="titledetails" fontname="Times New Roman" size="50"/>
        <labelfcol labelstylename="gridnum" fontname="Frankin Gothic Medium" size="40"/>

		<linestylecol linestyle="detail" strokewidth="0.8" strokecolour="#ff800080"/>
		<linestylecol linestyle="wall" strokewidth="3.5" strokecolour="#ff800080"/>
	</subsetattr>


	<subsetattr name="toplevel" uppersubset="default" areacolour="#88e50033" labelcolour="#ffe50033">
		<labelfcol labelstylename="qm" size="9.375"/>
		<labelfcol labelstylename="step"/>
		<labelfcol labelstylename="passage" fontstyle="BOLD" size="23" labelcolour="#ff00dfd0"/>
		<labelfcol labelstylename="area"/>
	</subsetattr>
	<set uppersubset="toplevel">
		<subsetattr name="suspendedsolution"/>
		<subsetattr name="livingdaylight"/>
		<subsetattr name="204d"/>
		<subsetattr name="204a"/>
		<subsetattr name="204f"/>
		<subsetattr name="204bent"/>
		<subsetattr name="trickortreat"/>
		<subsetattr name="stitchthis"/>
		<subsetattr name="germknodel"/>
		<subsetattr name="topofcrowning"/>
	</set>

	<subsetattr name="topplane" uppersubset="default" areacolour="#88ff9900" labelcolour="#ffff9900">
		<labelfcol labelstylename="qm"/>
		<labelfcol labelstylename="step"/>
		<labelfcol labelstylename="passage"/>
		<labelfcol labelstylename="area"/>
	</subsetattr>
	<set uppersubset="topplane">
		<subsetattr name="dutchbeauty"/>
		<subsetattr name="swings2cresta"/>
		<subsetattr name="ermintrude"/>
		<subsetattr name="juicybits"/>
		<subsetattr name="magicroundabout"/>
		<subsetattr name="mgrpitch1"/>
		<subsetattr name="theslide"/>
		<subsetattr name="southcircular"/>
		<subsetattr name="swings"/>
		<subsetattr name="chicargochicanes"/>
		<subsetattr name="takingthepiss"/>
		<subsetattr name="rhinorift"/>
		<subsetattr name="trapeze"/>
	</set>

	<subsetattr name="bottomplane" uppersubset="default" areacolour="#88ccff00" labelcolour="#ffccff00">
		<labelfcol labelstylename="qm"/>
		<labelfcol labelstylename="step"/>
		<labelfcol labelstylename="passage"/>
		<labelfcol labelstylename="area"/>
	</subsetattr>

	<subsetattr name="ariston" uppersubset="default" areacolour="#8819ff2d" labelcolour="#ff19ff2d">
		<labelfcol labelstylename="qm"/>
		<labelfcol labelstylename="step"/>
		<labelfcol labelstylename="passage"/>
		<labelfcol labelstylename="area"/>
	</subsetattr>

	<subsetattr name="gaffered" uppersubset="default" areacolour="#880033ff" labelcolour="#ff0033ff">
		<labelfcol labelstylename="qm"/>
		<labelfcol labelstylename="step"/>
		<labelfcol labelstylename="passage"/>
		<labelfcol labelstylename="area"/>
	</subsetattr>

	<subsetattr name="razor" uppersubset="default" areacolour="#88a900e5" labelcolour="#88a900e5">
		<labelfcol labelstylename="qm" labelcolour="none"/>
		<labelfcol labelstylename="step"/>
		<labelfcol labelstylename="passage"/>
		<labelfcol labelstylename="area"/>
		<linestylecol linestyle="pitchbound" strokewidth="0.725" spikegap="2.5" gapleng="0.0" spikeheight="3.5"  strokecolour="#ffb01010"/>
	</subsetattr>

</groupsubsetattr>

<area_signal_def asigname="plan" asigeffect="0"/>
<area_signal_def asigname="rock" asigeffect="1"/>
<area_signal_def asigname="hole" asigeffect="2"/>
<area_signal_def asigname="verysteep" asigeffect="0"/>

</fontcolours>
</tunnelxml>
